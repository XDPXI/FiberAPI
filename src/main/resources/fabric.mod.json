{
	"schemaVersion": 1,
	"id": "fiber-api",
	"version": "${version}",
	"name": "XD's Fiber API",
	"description": "FiberAPI is a lightweight Fabric API for XDPXI's mods, focusing on shared tools, compatibility, and performance.",
	"authors": [
		{
			"name": "XDPXI",
			"contact": {
				"homepage": "https://xdpxi.vercel.app"
			}
		}
	],
	"license": "Apache-2.0",
	"icon": "assets/fiber-api/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
          "dev.xdpxi.fiberapi.Main"
		],
		"client": [
			"dev.xdpxi.fiberapi.Client"
		]
	},
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": ">=1.21",
		"java": ">=21",
		"fabric-api": "*"
	},
	"recommends": {
		"xdlib": "*"
	},
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.modrinth": "https://modrinth.com/plugin/xdlib"
			},
			"badges": [ "library" ],
			"update_checker": true
		}
	}
}